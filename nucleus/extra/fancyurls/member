// default to default implementation (/member/1234)
// if memberid was not filled out by plugins
if ($memberid == 0)
{
	$data = i18n::explode("/",serverVar('PATH_INFO'));
	if (count($data) > 1)
	{
		$memberid = intval($data[1]);
	}
}

