<?php

include('./fancyurls.config.php');
include('./config.php');

// default to default implementation (/archive/1/2005-12)
// if blogid was not filled out by plugins
if ( ($blogid == 0) && ($archive == '') )
{
	$data = preg_split("#/#", serverVar('PATH_INFO'));
	if ( count($data) > 2 )
	{
		$blogid = (integer) $data[1];
		$archive = $data[2];
	}
}

selector();
